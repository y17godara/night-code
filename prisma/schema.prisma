generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mongodb"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
}

model TotalRequest {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  count Int
}